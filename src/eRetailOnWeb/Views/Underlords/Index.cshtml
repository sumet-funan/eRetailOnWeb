@model eProductOnWeb.Models.Underlords.Ranking

@{
    ViewData["Title"] = "Underlords Ranking";
}

<ul class="nav nav-tabs" id="tabUnderlords" role="tablist">
    <li class="nav-item">
        <a class="nav-link active" id="ranking-tab" data-toggle="tab" href="#contentRanking" role="tab" aria-controls="contentRanking" aria-selected="true">Ranking</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" id="player-tab" data-toggle="tab" href="#contentPlayer" role="tab" aria-controls="contentPlayer" aria-selected="false">Player</a>
    </li>
</ul>
<div class="tab-content" id="tabContentUnderlords">
    <div class="tab-pane fade show active" id="contentRanking" role="tabpanel">
        <div class="card-body">
            <partial name="_rankingTable" model="@Model" />
        </div>
    </div>
    <div class="tab-pane fade" id="contentPlayer" role="tabpanel">
        <p>
            <div class="card card-body">Player</div>
        </p>
    </div>
</div>